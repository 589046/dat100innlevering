package obligInnlevering;

import java.util.Scanner;

public class Oppgåve4 {
	public static void main(String[] args) {
		Scanner inn = new Scanner (System.in);
		int lønn = inn.nextInt();
		
		if (lønn < 180800) {						//reknar ut grunnskatt
			double trinn0 = lønn * 0.22;
			System.out.println("hei1");
			
		} if (lønn >= 180800 && lønn < 254500) { 	//reknar ut skatten for trinn1 
			double trinn0 = 180800 * 0.22;
			lønn = lønn-180800;
			double trinn1 = (lønn*0.239);
			System.out.println(trinn0);
			System.out.println(trinn1);
			
		} if (lønn >= 254500 && lønn < 639750) {	//reknar ut skatten for trinn2
			double trinn0 = 180800 * 0.22;
			lønn = lønn-180800;
			
			double trinn1 = 73700 * 0.239;
			lønn = lønn-73700;
			double trinn2 = (lønn-73700)*0.262;
			
			System.out.println(trinn0);
			System.out.println(trinn1);
			System.out.println(trinn2);
		}
		if (lønn >= 639750 && lønn < 999550) { 		//reknar ut skatten for trinn3
			double trinn0 = 180800 * 0.22;
			lønn = lønn-180800;
			
			double trinn1 = 73700*0.239;
			lønn = lønn - 73700;
			
			double trinn2 = 385250*0.262;
			lønn = lønn - 385250;
			
			double trinn3 = lønn*0.352;
			
			System.out.println(trinn0);
			System.out.println(trinn1);
			System.out.println(trinn2);
			System.out.println(trinn3);
		}
		if (lønn >= 999550) {						//reknar ut skatten for trinn4
			double trinn0 = 180800 * 0.22;
			lønn = lønn-180800;
			
			double trinn1 = 73700*0.239;
			lønn = lønn - 73700;
			
			double trinn2 = 385250*0.262;
			lønn = lønn - 385250;
			
			double trinn3 = 359750*0.352;
			lønn = lønn - 359800;
			
			double trinn4 = lønn*0.382;
			
			System.out.println(lønn);
			
			System.out.println(trinn0);
			System.out.println(trinn1);
			System.out.println(trinn2);
			System.out.println(trinn3);
			System.out.println(trinn4);
		}
		double skatt = trinn0 + trinn1 + trinn2 + trinn3 + trinn4;
	}
}
